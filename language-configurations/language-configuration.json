{
  "comments": {
    "lineComment": {
      "comment": "//"
    },
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "'",
      "close": "'",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string"
      ]
    },
    {
      "open": "`",
      "close": "`",
      "notIn": [
        "string",
        "comment"
      ]
    },
    {
      "open": "/**",
      "close": " */",
      "notIn": [
        "string"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "'",
      "'"
    ],
    [
      "\"",
      "\""
    ],
    [
      "`",
      "`"
    ]
  ],
  "folding": {
    "markers": {
      "start": "//\\s*(MARK|#region):\\s*",
      "end": "//\\s*endregion\\b"
    }
  }
}