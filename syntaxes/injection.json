{
  "scopeName": "html.alpinejs.attribute",
  "injectionSelector": [
    "L:text.html.derivative -comment",
    "L:text.html.blade -comment",
    "L:text.html.jinja -comment",
    "L:text.html.liquid -comment",
    "L:text.html.nunjucks -comment",
    "L:text.html.njk -comment",
    "L:text.html.php -comment",
    "L:text.html.twig -comment"
  ],
  "patterns": [
    {
      "include": "#alpine-directives"
    }
  ],
  "repository": {
    "alpine-directives": {
      "patterns": [
        {
          "name": "meta.attribute.alpine",
          "begin": "(?:\\b(x-)|(:|@|#))([a-zA-Z0-9\\-_]+)(?:(\\:[a-zA-Z\\-_]+))?((?:\\.[a-zA-Z0-9\\-_]+)+)?\\s*(=)",
          "end": "(?<=\")|(?=[\\s<>`])",
          "captures": {
            "1": {
              "name": "entity.other.attribute-name.html"
            },
            "2": {
              "name": "entity.other.attribute-name.html"
            },
            "3": {
              "name": "entity.other.attribute-name.html"
            },
            "4": {
              "name": "entity.other.attribute-name.html"
            },
            "5": {
              "name": "entity.other.attribute-name.html"
            },
            "6": {
              "name": "punctuation.separator.key-value.html"
            }
          },
          "patterns": [
            {
              "begin": "\"",
              "end": "\"",
              "beginCaptures": {
                "0": {
                  "name": "string.quoted.double.html punctuation.definition.string.begin.html"
                }
              },
              "endCaptures": {
                "0": {
                  "name": "string.quoted.double.html punctuation.definition.string.end.html"
                }
              },
              "patterns": [
                {
                  "begin": "\\{?(?=\\s*(?://|/\\*)\\s*(?:#region|MARK:)?\\s*(?:javascript|js|JS))",
                  "end": "(?=\")",
                  "contentName": "meta.embedded.inline.alpinejs",
                  "patterns": [
                    {
                      "include": "source.js"
                    }
                  ]
                },
                {
                  "begin": "\\{#",
                  "end": "#\\}",
                  "name": "comment.block"
                },
                {
                  "begin": "\\{\\{-?",
                  "end": "-?\\}\\}",
                  "name": "meta.tag.template.value.twig",
                  "beginCaptures": {
                    "0": {
                      "name": "punctuation.section.tag.twig"
                    }
                  },
                  "endCaptures": {
                    "0": {
                      "name": "punctuation.section.tag.twig"
                    }
                  },
                  "patterns": [
                    {
                      "include": "text.html.twig#twig-constants"
                    },
                    {
                      "include": "text.html.twig#twig-operators"
                    },
                    {
                      "include": "text.html.twig#twig-functions-warg"
                    },
                    {
                      "include": "text.html.twig#twig-functions"
                    },
                    {
                      "include": "text.html.twig#twig-macros"
                    },
                    {
                      "include": "text.html.twig#twig-objects"
                    },
                    {
                      "include": "text.html.twig#twig-properties"
                    },
                    {
                      "include": "text.html.twig#twig-filters-warg"
                    },
                    {
                      "include": "text.html.twig#twig-filters"
                    },
                    {
                      "include": "text.html.twig#twig-filters-warg-ud"
                    },
                    {
                      "include": "text.html.twig#twig-filters-ud"
                    },
                    {
                      "include": "text.html.twig#twig-strings"
                    },
                    {
                      "include": "text.html.twig#twig-arrays"
                    },
                    {
                      "include": "text.html.twig#twig-hashes"
                    }
                  ]
                },
                {
                  "begin": "\\{%-?",
                  "end": "-?%\\}",
                  "name": "meta.tag.template.block.twig",
                  "beginCaptures": {
                    "0": {
                      "name": "punctuation.section.tag.twig"
                    }
                  },
                  "endCaptures": {
                    "0": {
                      "name": "punctuation.section.tag.twig"
                    }
                  },
                  "patterns": [
                    {
                      "include": "text.html.twig#twig-constants"
                    },
                    {
                      "include": "text.html.twig#twig-keywords"
                    },
                    {
                      "include": "text.html.twig#twig-operators"
                    },
                    {
                      "include": "text.html.twig#twig-functions-warg"
                    },
                    {
                      "include": "text.html.twig#twig-functions"
                    },
                    {
                      "include": "text.html.twig#twig-macros"
                    },
                    {
                      "include": "text.html.twig#twig-filters-warg"
                    },
                    {
                      "include": "text.html.twig#twig-filters"
                    },
                    {
                      "include": "text.html.twig#twig-filters-warg-ud"
                    },
                    {
                      "include": "text.html.twig#twig-filters-ud"
                    },
                    {
                      "include": "text.html.twig#twig-objects"
                    },
                    {
                      "include": "text.html.twig#twig-properties"
                    },
                    {
                      "include": "text.html.twig#twig-strings"
                    },
                    {
                      "include": "text.html.twig#twig-arrays"
                    },
                    {
                      "include": "text.html.twig#twig-hashes"
                    }
                  ]
                },
                {
                  "match": "([^\"]*?)((?:(?<!:)//|/\\*).*)",
                  "captures": {
                    "1": {
                      "name": "string.quoted.double.html"
                    },
                    "2": {
                      "name": "comment.line.double-slash.js"
                    }
                  }
                },
                {
                  "match": "[^\"\\{]+|\\{(?!%|#|\\{)",
                  "name": "string.quoted.double.html"
                }
              ]
            }
          ]
        },
        {
          "name": "meta.attribute.alpine",
          "match": "(?<!-)\\b(x-)([a-zA-Z0-9\\-_]+)(?:(\\:[a-zA-Z\\-_]+))?((?:\\.[a-zA-Z0-9\\-_]+)+)?(?=\\s|>|/>)",
          "captures": {
            "1": {
              "name": "entity.other.attribute-name.html"
            },
            "2": {
              "name": "entity.other.attribute-name.html"
            },
            "3": {
              "name": "entity.other.attribute-name.html"
            },
            "4": {
              "name": "entity.other.attribute-name.html"
            }
          }
        }
      ]
    }
  }
}